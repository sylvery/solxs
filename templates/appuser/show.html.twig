{% extends 'base.html.twig' %}

{% block title %}Appuser{% endblock %}

{% block body %}
    <h1>Appuser</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appuser.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ appuser.username }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ appuser.roles ? appuser.roles|json_encode : '' }}</td>
            </tr>
